{
  "version": 2,
  "builds": [
    {
      "src": "api/index.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/app",
      "dest": "/static/app.html"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.go"
    }
  ],
  "outputDirectory": "static"
}
